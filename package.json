{
  "name": "d1-pvp-csv-dump",
  "version": "1.0.0",
  "description": "Dump Destiny 1 PVP reports via Node command line",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon ./src/index.ts",
    "exec": "node build/index.js",
    "once": "npm run build && npm run exec",
    "build": "tsc -p tsconfig.json",
    "dist": "tsc -p tsconfig.json -outDir dist --sourceMap false",
    "clean": "rm -rf dist build",
    "ts-node": "ts-node",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Destiny"
  ],
  "author": "Dave Caslin",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.2.0",
    "nodemon": "^2.0.14",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "dotenv": "^10.0.0"
  }
}
